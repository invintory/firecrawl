meta {
  name: Search and optionally scrape search results
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/search
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "query": "Caymus Cabernet Sauvignon 2014 price -site:wine-searcher.com",
    "limit": 5,
  //   "tbs": "",
  //   "location": "",
    "timeout": 30000,
    "ignoreInvalidURLs": true,
    "scrapeOptions": {
      "formats": ["markdown"]
    }
  }
}
