meta {
  name: Scrape a single URL and optionally extract information using an LLM
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/scrape
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "url": "https://www.wine-searcher.com/find/sea+smoke+chard+santa+rita+hill+barbara+county+central+coast+california+usa/2021/usa",
    "formats": ["markdown"],
    "onlyMainContent": true,
    "blockAds": true,
    "removeBase64Images": true,
    "timeout": 30000
  }
}
